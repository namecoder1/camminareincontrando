---
import Footer from '../components/Footer.astro';
import Navbar from '../components/Navbar.astro';

const {
  title = 'Camminare Incontrando',
  description,
  keywords = 'benessere, fisioterapia, miglioramento',
  robots = 'index, follow',
  ogTitle,
  ogDescription,
  ogType,
  ogUrl,
  pubTime,
  modTime
} = Astro.props;

interface Props {
  title: string;
  ogTitle: string;
  ogDescription: string;
  description: string;
  keywords?: string;
  robots?: string;
  ogType: string;
  ogUrl?: string;
  pubTime: string;
  modTime: string;
}
---

<html lang="it">
    <!--
    Coded by Tobia Bartolomei
    https://tobiabartolomei.com
    -->
  <head>
    <script type="text/javascript">
      var _iub = _iub || [];
      _iub.csConfiguration = {"askConsentAtCookiePolicyUpdate":true,"floatingPreferencesButtonColor":"#FFFFFF","floatingPreferencesButtonDisplay":"bottom-right","perPurposeConsent":true,"siteId":3664568,"whitelabel":false,"cookiePolicyId":56085812,"lang":"it", "banner":{ "acceptButtonCaptionColor":"#FFFFFF","acceptButtonColor":"#0073CE","acceptButtonDisplay":true,"backgroundColor":"#FFFFFF","closeButtonRejects":true,"customizeButtonCaptionColor":"#4D4D4D","customizeButtonColor":"#DADADA","customizeButtonDisplay":true,"explicitWithdrawal":true,"listPurposes":true,"position":"float-bottom-right","showTitle":false,"textColor":"#000000" }};
      </script>
      <script type="text/javascript" src="https://cs.iubenda.com/autoblocking/3664568.js"></script>
      <script type="text/javascript" src="//cdn.iubenda.com/cs/iubenda_cs.js" charset="UTF-8" async></script>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <meta name="description" content={description} />
		<meta name="robots" content={robots} />
    <meta name="author" content="Christian Cesaroni" />
    <meta name="keywords" content={keywords} />
    <meta name="article:published_time" content={pubTime} >
    <meta name="article:modified_time" content={modTime} >
		<!-- OG Tags  -->
		<meta name="og:title" content={ogTitle} />
    <meta name="og:type" content={ogType} />
    <meta name="og:description" content={ogDescription} />
    <meta name="og:site_name" content="Camminare Incontrando" />
    <meta name="og:url" content={'https://ciblog24.netlify.app' + ogUrl} />
    <meta name="og:image" content="https://ciblog24.netlify.app/ogImage.png" />
		<!-- Fonts & Icons -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
    <script
      src="https://kit.fontawesome.com/589b186945.js"
      crossorigin="anonymous"></script>
    <title>{title} | Camminare Incontrando</title>
  </head>
  <body class="font-poppins">
    <Navbar />
    <main>
      <slot />
    </main>
    <Footer />
  </body>
</html>
